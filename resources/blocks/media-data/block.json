{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "x3p0/media-data",
	"version": "1.0.0",
	"title": "Media Data",
	"category": "media",
	"icon": "info-outline",
	"description": "Display data for media attachments.",
	"keywords": ["media", "metadata", "attachment", "exif"],
	"textdomain": "x3p0-media-data",
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"__experimentalStyle": {
			"css": "&.is-layout-flex { gap: var(--wp--custom--x-3-p-0-media-data--gap, 0.5rem); }",
			"spacing": {
				"blockGap": "var(--wp--custom--x-3-p-0-media-meta--gap, 0.5rem)"
			}
		},
		"layout": {
			"allowCustomContentAndWideSize": false,
			"allowEditing": false,
			"allowInheriting": false,
			"allowJustification": true,
			"allowOrientation": false,
			"allowSizingOnChildren": false,
			"allowSwitching": true,
			"allowVerticalAlignment": true,
			"default": {
				"type": "constrained"
			}
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		},
		"color": {
			"text": true,
			"background": true,
			"link": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		}
	},
	"providesContext": {
		"x3p0-media-data/mediaId": "mediaId",
		"x3p0-media-data/metadata": "metadata"
	},
	"usesContext": ["postId", "postType"],
	"attributes": {
		"mediaId": {
			"type": "integer",
			"default": 0,
			"role": "content"
		}
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
